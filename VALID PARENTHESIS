import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner in= new Scanner(System.in);
        
        String que=in.nextLine();
        
    Stack<Character> stack= new Stack <>();
    
    for(char ch:que.toCharArray()){
        if(ch=='('||ch=='['||ch=='{'){
            stack.push(ch);
        }
        else{
            if(stack.isEmpty()){
                System.out.println("false");
                return;
            }
    
    
    
    char top=stack.pop();
    
    if(ch==')' && top!='(') {
        System.out.println("false");
        return;
    }
    else if(ch=='}' && top!='{'){
         System.out.println("false");
         return;
    }
    else if(ch==']' && top!='['){
          System.out.println("false");
          return;
    }
        }
    }
    
    if(stack.isEmpty()){
        System.out.println("true");
    }
    else{
        System.out.println("false");
    }
}

}



